def factors(x):
    e=[]
    for i in range(1,x+1):
        if x%i==0:
            e.append(i)
    return e
def fun():
    opened=[]
    x=int(input("Enter the number of lockers in a school: "))
    for locker in range(1,x+1):
        
        if not len(factors(locker))%2==0:
            opened.append("Its opened")
    o=len(opened)
    return o
            
